<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.yj.beanfactory.ThresholdRequiredBeanFactoryPostProcessor">
        <property name="defaultThreshold" value="17"/>
    </bean>

    <bean id="collector1" class="com.yj.beanfactory.DataCollector">
        <property name="threshold" value="10"/>
    </bean>

    <bean id="collector2" class="com.yj.beanfactory.DataCollector"/>
</beans>